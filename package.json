{
  "name": "redux-via-decorators",
  "version": "0.0.1",
  "private": false,
  "description": "A library for making development with redux more comfortable and efficient via decorators.",
  "keywords": [
    "redux",
    "decorators",
    "decorator",
    "reducer",
    "form",
    "redux-form",
    "react",
    "react-redux",
    "flux",
    "saga",
    "submit"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:qens/redux-decorators.git"
  },
  "author": "Dan Bul <qens007@gmail.com>",
  "bugs": {
    "url": "https://github.com/qens/redux-decorators/issues"
  },
  "scripts": {
    "test": "karma start",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "cross-env BABEL_ENV=development babel src/ --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src/ --out-dir es",
    "prepare": "npm run build"
  },
  "dependencies": {
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-form-saga": "^0.2.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "immutable": "^3.8.2",
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "cross-env": "^5.1.3",
    "expect": "^22.1.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.9",
    "mocha": "^4.1.0",
    "react-hot-loader": "^3.1.3",
    "webpack": "^2.1.0-beta.22",
    "webpack-config": "^7.0.0",
    "webpack-dev-middleware": "^1.7.0",
    "webpack-hot-middleware": "^2.12.2"
  },
  "license": "MIT"
}
